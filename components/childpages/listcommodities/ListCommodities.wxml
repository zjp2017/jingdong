<!--components/childpages/listcommodities/ListCommodities.wxml-->
<view class="list-warp">
	<!-- 1.列表项目 -->
	<view class="list-warp-item">
		<view class="item-left">
			<image class="item-left-logo" src="https://img30.360buyimg.com/vendersettle/s120x120_jfs/t1/28439/30/9372/12503/5c7e4be9E765cc841/d1f3e78254d4488f.png"></image>
		</view>
		<view class="item-right">
			<view class="store-name">沃尔玛惠选-南山华夏路店</view>
			<view class="store-content">
				<view class="store-content-left">
					<view class="">
						<!-- <image src="data:image/png;base64,"></image> -->
					</view>
					<view class="score-num">4.6分</view>
					<view class="sale-num">月销8千件+</view>
				</view>
				<view class="store-content-right">
					<view class="store-content-right-time">51分钟</view>
					<view class="store-content-right-Division">|</view>
					<view class="store-content-right-range">4.8km</view>
				</view>
			</view>
			<view class="store-content-info">
				<view class="store-content-info-left">
					<view class="store-send-condition">起送￥0</view>
					<view class="store-content-right-Division">|</view>
					<view class="store-send-free">基础运费￥8</view>
				</view>
				<view>
					<view class="store-send-brand">达达专送</view>
				</view>
			</view>
			<!--优惠券  -->
			<view class="coupon-warp">
				<view class="coupon-box" style="height:{{direFlg==0?'32rpx':'auto'}}">
					<view class="coupon-item">领25元券</view>
					<view class="coupon-item">兑5元红包</view>
					<view class="coupon-item">兑5元红包</view>
					<view class="coupon-item">领10元叠加券</view>
					<view class="coupon-item">领10元叠加券</view>
					<view class="coupon-item">领4元券</view>
					<view class="coupon-item">领4元券</view>
					<view class="coupon-item">领4元券</view>
					<view class="gift-warp-main" >
						<view class="gift-warp-title">返礼</view>
						<view class="gift-warp-content">买车厘子送由集蛋糕券，适用范围见券说明</view>
					</view>
					<view class="gift-warp-main" >
						<view class="gift-warp-title reduce">满减</view>
						<view class="gift-warp-content">心相印卷纸满60减30.8</view>
					</view>
					<view class="gift-warp-main" >
						<view class="gift-warp-title postage">运费</view>
						<view class="gift-warp-content">商家会员满89元减4元运费(每月2次),满99元减4元运费(每月2次)</view>
					</view>
					<view class="gift-warp-main">
						<view class="gift-warp-title">返礼</view>
						<view class="gift-warp-content">买车厘子送由集蛋糕券，适用范围见券说明</view>
					</view>
				</view>
				<!--  -->
				
				<image class="coupon-box-icon" src="{{direFlg==0?'../../../static/images/home/down_icon2.png':'../../../static/images/home/up_icon.png'}}" bind:tap="getMore"></image>
			</view>
			<view class="gift-warp" wx:if="{{showGift}}">
				<view class="gift-warp-title">返礼</view>
				<view class="gift-warp-content">买车厘子送由集蛋糕券，适用范围见券说明</view>
			</view>
			<!-- 列表中的商品样式 -->
			<searcheditem-com></searcheditem-com>
		</view>
	</view>
	<!-- 2 -->
	
</view>
